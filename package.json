{
	"name": "n8n-nodes-blossom",
	"version": "1.0.1",
	"description": "n8n node for Blossom API integration - Generic integration for any Blossom instance",
	"license": "MIT",
	"homepage": "https://github.com/yourusername/n8n-nodes-blossom",
	"keywords": [
		"n8n-community-node-package",
		"blossom",
		"api",
		"integration",
		"generic"
	],
	"author": {
		"name": "Erez",
		"email": "kiai66@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/n8n-nodes-blossom.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/BlossomApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Blossom/Blossom.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
